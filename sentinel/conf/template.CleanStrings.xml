<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">

<!-- $Header: /share/content/gforge/sentinel/sentinel/conf/template.CleanStrings.xml,v 1.2 2008-11-07 14:11:11 hebell Exp $
       $Name: not supported by cvs2svn $
 -->

<properties>
    
        <!-- Specify the caDSR database being used by the Sentinel Tool.
        -->
        <entry key="DSurl">@CADSR.TOOL.URL@</entry>
        <entry key="DSusername">@CADSR.TOOL.USER@</entry>
        <entry key="DSpassword">@CADSR.TOOL.PSWD@</entry>
        
        <!-- Use "select" to see the records that will be updated, use "update" to clean up the data -->
        <entry key="action">select</entry>
        
        <entry key="table.01">sbr.data_elements_view long_name preferred_definition</entry>
        <entry key="table.02">sbr.data_element_concepts_view long_name preferred_definition</entry>
        <entry key="table.03">sbr.admin_components_view long_name preferred_definition</entry>
</properties>
